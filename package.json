{
  "name": "flow-to-typescript-codemod",
  "version": "0.0.0",
  "private": true,
  "description": "Stripe's codemod for converting Flow to TypeScript.",
  "repository": "https://github.com/stripe-archive/flow-to-typescript-codemod",
  "license": "MIT",
  "author": "Stripe, Inc.",
  "types": "./flow.d.ts",
  "bin": {
    "typescriptify": "node ./bin.js"
  },
  "files": [
    "dist",
    "flow.d.ts",
    "bin.js"
  ],
  "scripts": {
    "build": "node ./esBuild.js",
    "dev": "yarn build && node ./dist/index.js",
    "typescriptify": "yarn build && node ./dist/index.js",
    "lint": "eslint --ext ts src",
    "prepare": "yarn patch-package",
    "prettier": "prettier --write 'src/**/*.{js,jsx,mdx,ts,tsx}' !src/test/test_files/*.{js,jsx,ts,tsx}",
    "test": "jest",
    "types": "tsc -b ."
  },
  "resolutions": {
    "@babel/parser": "7.16.0",
    "@babel/traverse": "7.16.0",
    "@babel/types": "7.16.0",
    "code-block-writer": "10",
    "minimist": "1.2.6"
  },
  "devDependencies": {
    "@babel/parser": "7.20.3",
    "@babel/traverse": "7.20.1",
    "@babel/types": "7.20.2",
    "@types/babel__traverse": "^7.18.2",
    "@types/csv-stringify": "^3.1.0",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^12.20.55",
    "@types/react": "^16.14.34",
    "@types/signale": "^1.4.4",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "chalk": "^2.4.2",
    "csv-stringify": "^6.0.4",
    "dedent": "^0.7.0",
    "esbuild": "0.15.15",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "flow-bin": "~0.188",
    "fs-extra": "^8.1.0",
    "ignore": "^5.2.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.3.0",
    "patch-package": "^6.5.0",
    "prettier": "^2.7.1",
    "recast": "^0.20.5",
    "signale": "^1.4.0",
    "ts-jest": "^26.5.6",
    "ts-morph": "^17.0.0",
    "typescript": "4.9.3",
    "yargs": "^17.6.2"
  },
  "packageManager": "yarn@3.3.0"
}
